		xmlns:oa="http://www.openapplications.org/oagis/9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<!-- This is an example of an instruction to retrieve up to 30 instances of IndicativePersonDossier --> 
 <oa:ApplicationArea>
        <oa:CreationDateTime>2009-08-13</oa:CreationDateTime>
    </oa:ApplicationArea>
    <DataArea>
        <oa:Get uniqueIndicator="true" recordSetSaveIndicator="true" maxItems="30">
            <oa:Expression expressionLanguage="XPath">/GetIndicativeData/DataArea/IndicativeData/IndicativePersonDossier</oa:Expression>
        </oa:Get>
        <IndicativeData majorVersionID="1" minorVersionID="0">
            <IndicativePersonDossier>
                <IndicativeDeployment>
                    <ResourceRelationshipCode>Vendor Employee</ResourceRelationshipCode>
                </IndicativeDeployment>
            </IndicativePersonDossier>
        </IndicativeData>
    </DataArea> 
</GetIndicativeData>
